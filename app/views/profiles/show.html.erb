<div class="page-header">
	<h1><%= @user.profile_name %></h1>
	<hr/>
</div>
<% if @reviews %>
	<% @reviews.each do |review|%>
	<%=review.inspect%>
		<div class="index">
		<h4><%= review.title%></h4> <br />	
		<%= review.content%>		<hr />
		<%= review.venue_id%>
		<%= link_to time_ago_in_words(review.created_at), review_path(review) %> ago
		</div>
	<%end%>
<%end%>



<p>Find me in app/views/profiles/show.html.erb</p>
